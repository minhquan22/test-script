local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Test Hub", HidePremium = false, IntroText = "Test Hub", SaveConfig = true, ConfigFolder = "OrionTest"})

--Tabs
local Tab = Window:MakeTab({
    Name = "Main",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

Tab:AddButton({
    Name = "white screen on",
    Callback = function()
        print("button pressed")
        local RunService = game:GetService("RunService")
        RunService:Set3dRenderingEnabled(false)
        setfpscap(15)
    end
})

Tab:AddButton({
    Name = "white screen off",
    Callback = function()
        print("button pressed")
        local RunService = game:GetService("RunService")
        RunService:Set3dRenderingEnabled(true)
        setfpscap(15)
    end
})

--Toggles
Tab:AddToggle({
    Name = "Fast Attack",
    Default = false,
    Callback = function(Value)
        local SuperFastMode = false -- Change to true if you want Super Super Super Fast attack (Like instant kill) but it will make the game kick you more than normal mode
        local plr = game.Players.LocalPlayer
        local CbFw = debug.getupvalues(require(plr.PlayerScripts.CombatFramework))
        local CbFw2 = CbFw[2]

        -- ... (rest of the code)

        local Section = Tab:AddSection({
            Name = "FastAttack :"
        })

        _G.AttackNoCD = Value
    end
})
